{
  "name": "nlr-skeleton",
  "version": "0.0.1",
  "description": "NarraLeaf-React Skeleton",
  "packageManager": "yarn@4.4.1",
  "bin": "./dist/index.js",
  "scripts": {
    "build:dev": "esbuild src/index.ts --bundle --platform=node --format=cjs --outfile=dist/index.js",
    "build": "esbuild src/index.ts --bundle --platform=node --format=cjs --outfile=dist/index.js --minify"
  },
  "devDependencies": {
    "@types/node": "^22.13.4",
    "@types/react": "^19",
    "chalk": "^5.4.1",
    "esbuild": "^0.19.0",
    "narraleaf-react": "^0.4.1",
    "react": "^19.0.0",
    "typescript": "^5.0.0"
  },
  "installConfig": {
    "pnp": false
  },
  "files": [
    "package.json",
    "LICENSE",
    "dist/",
    "skeleton/"
  ]
}
